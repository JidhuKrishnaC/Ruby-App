

<table class="table table-striped table-bordered table-hover">
<thead style="background-color: #01BD95;">
<tr>
<th><%= t('global.table.first_name') %></th>
<th><%= t('global.table.last_name') %></th>
<th><%= t('global.table.email') %></th>
<th><%= t('global.table.dob') %></th>
<th colspan="3"></th>
</tr>
</thead>



<tbody id="users">
    <tr>
    <%= render @user %>
      <td><%= link_to t('global.forms.edit_developer'), edit_user_path(@user) %></td>
      <td><%= link_to t('global.forms.back_developer'), users_path %></td>
    </tr>
</tbody>
</table>

<div>
  <%= button_to t('global.forms.destroy_developer'), @user, method: :delete, class:"btn btn-success" %>
</div>
